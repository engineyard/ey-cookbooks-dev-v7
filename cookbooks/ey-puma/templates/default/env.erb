current_path="/data/<%= @app_name %>/current"
pid_directory="/var/run/engineyard/<%= @app_name %>/"
log_directory="/var/log/engineyard/apps/<%= @app_name %>/"
framework_env="<%= @framework_env %>"
baseport="<%= @baseport %>"
workers="<%= @workers %>"
PATH="/data/<%= @app_name %>/current/ey_bundler_binstubs:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games"
threads="<%= @threads %>"
