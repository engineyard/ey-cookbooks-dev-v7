---
:keep: <%= @keep %>
:aws_secret_id: <%= @id %>
:aws_secret_key: <%= @key %>
:region: <%= @region %>
:backup_bucket: <%= @backup_bucket %>
:dbuser: <%= @dbuser %>
:dbpass: <%= @dbpass %>
<% if @log_coordinates %>
:log_coordinates: <%= @log_coordinates %>
<% end %>
:env: <%= @env %>
:databases:
<% @databases.each do |database| %>
<%= "- #{database}" %>
<% end %>
